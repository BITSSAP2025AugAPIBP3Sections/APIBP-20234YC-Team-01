        }
      },
      "get": {
        "tags": ["Donation Management"],
        "summary": "List donations",
        "description": "Lists all donations with optional filters by donor, recipient, or status. Access by [ADMIN, DONOR, RECIPIENT]",
        "operationId": "listDonations",
        "parameters": [
          {
            "name": "donorId",
            "in": "query",
