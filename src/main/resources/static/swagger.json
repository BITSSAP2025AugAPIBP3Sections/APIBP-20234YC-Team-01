    "/api/v1/food/browseAvailableFood": {
      "get": {
        "tags": ["Food Management"],
        "summary": "Browse Available Food",
        "description": "Allows recipients to browse available food listings.",
        "operationId": "browseAvailableFood",
        "responses": {
          "200": {
            "description": "Available food items",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Food"
                  }
                }
              }
            }
          },
          "204": {
            "description": "No food items currently available"
          }
        }
      }
    },

    "/api/v1/food/browseFoodRequests": {
      "get": {
        "tags": ["Food Management"],
        "summary": "Browse Food Requests",
        "description": "Allows donors to browse active food requests.",
        "operationId": "browseFoodRequests",
        "responses": {
          "200": {
            "description": "Food requests fetched successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/FoodRequest"
                  }
                }
              }
            }
          },
          "204": {
            "description": "No active food requests found"
          }
        }
      }
    },

    "/api/v1/food/filterFood": {
      "get": {
        "tags": ["Food Management"],
        "summary": "Filter Food",
        "description": "Allows donors or recipient to filter food listings by category, expiry date, and location.",
        "operationId": "filterFood",
        "parameters": [
          {
            "name": "category",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "enum": ["VEG", "NONVEG", "JAIN", "WASTE"]
            }
          },
          {
            "name": "expiryAfter",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "location",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Filtered food items",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Food"
                  }
                }
              }
            }
          },
          "404": {
            "description": "No food items match the filter criteria",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "code": {
                      "type": "integer",
                      "example": 404
                    },
                    "message": {
                      "type": "string"
                    },
                    "suggestions": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    },
                    "timeStamp": {
                      "type": "string",
                      "format": "date-time"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },

